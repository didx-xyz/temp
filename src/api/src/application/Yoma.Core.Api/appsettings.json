{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AllowedOrigins": "[AllowedOrigins]",
  "ConnectionStrings": {
    "SQLConnection": "[SQLConnection]"
  },

  "AppSettings": {
    "AdminApiKey": "[AdminApiKey]",
    "AdminKeyCloak": {
      "Username": "[Username]",
      "Password": "[Password]"
    },
    "WebhookAdminKeyCloak": {
      "Username": "[Username]",
      "Password": "[Password]"
    },
    "SwaggerScopes": "[Scopes]",
    "AuthorizationPolicyScope":  "[Scope]",
    "CacheSlidingExpirationLookupInHours": "[Hours]",
    "CacheAbsoluteExpirationRelativeToNowLookupInDays": "[Days]",
    "CacheEnabledByReferenceDataTypes": "[Types]"
  },

  "Keycloak": {
    "realm": "[realm]",
    "auth-server-url": "[auth-server-url]",
    "ssl-required": "none",
    "resource": "[resource]",
    "verify-token-audience": true,
    "credentials": {
      "secret": "[secret]"
    },
    "use-resource-role-mappings": true,
    "confidential-port": 0
  },
  "AWSSettings": {
    "S3Region": "[region]",
    "S3AccessKey": "[key]",
    "S3SecretKey": "[secret]",
    "S3BucketName": "[bucketName]",
    "S3URLExpirationInMinutes": 60
  }
}
